{
  "version": 2,
  "builds": [
    {
      "src": "frontend/next.config.ts",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://todo-backend.onrender.com"  // Replace with your actual backend URL
  }
}